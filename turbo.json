{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:frontend": {
      "dependsOn": ["^build:frontend"],
      "outputs": ["dist/**"]
    },
    "build:backend": {
      "dependsOn": ["^build:backend"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "lint": {},
    "typecheck": {},
    "test": {}
  }
}
