  "devDependencies": {
    "@types/node": "^22.1.0",
    "@types/qs": "^6.9.15",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@zodyac/env": "^1.1.8",
    "@fastify/cors": "^9.0.1",
    "@fastify/http-proxy": "^9.5.0",
    "fastify": "^4.28.1",
    "js-base64": "^3.7.7",
    "qs": "^6.13.0",
    "zod": "^3.23.8"
  }
}
